{
  "name": "TwitchFighter",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "prebuild": "npm run buildswag",
    "build": "tsc --outDir dist/src src/server.ts",
    "buildswag": "swagger-jsdoc -d swaggerDef.js -o src/swagger.json",
    "prestart": "npm run build",
    "start": "node dist/src/server.js",
    "buildAngular": "ng build",
    "startAngular": "ng serve",
    "test": "nodemon --watch . --ext ts --exec \"mocha -r ts-node/register src/test/**/*.test.ts\"",
    "lint": "ng lint",
    "tslint": "tslint -c tslint.json -p tsconfig.json --fix",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@types/cors": "^2.8.6",
    "@types/mongodb": "^3.3.15",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-swagger-generator": "^1.1.17",
    "jasmine-spec-reporter": "^4.2.1",
    "lodash": "4.17.13",
    "mongodb": "^3.5.2",
    "mongoose": "^5.8.11",
    "protractor": "^5.4.3",
    "rxjs": "~6.4.0",
    "supertest": "^4.0.2",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.3",
    "tslib": "^1.10.0",
    "tslint": "^6.0.0",
    "zone.js": "~0.9.1"
